ame: 'XXX-Wiki', presets: [ [ ({ docs: { sidebarPath: require.resolve('./sidebars.js'), routeBasePath: '/', editUrl: // 主分支链接 'https://github.com/Neige7/XXX-Wiki/blob/main' }, }), ], ], themeConfig: ({ navbar: { // 上边栏标题 title: 'XXX', items: [ { // github链接 href: 'https://github.com/Neige7/XXX', label: 'GitHub', position: 'right', }, ], }, // 底部链接 footer: { style: 'dark', links: [ { title: '交流', items: [ { label: 'QQ群', // 加群链接 href: 'https://www.baidu.com', }, ], }, { title: '插件发布', items: [ { label: 'GitHub', // github链接 href: 'https://github.com/Neige7/XXX', }, ], }, ], // 底部版权信息 copyright: `Copyright © ${new Date().getFullYear()} Neige, All Rights Reserved.`, }, }), }; module.exports = config;","s":"docusaurus.config.js","u":"/Wiki-Template/更改各种路径","h":"#docusaurusconfigjs","p":13},{"i":18,"t":"{ // 名称 \"name\": \"xxx-wiki\", // 版本 \"version\": \"0.0.0\", }","s":"package.json","u":"/Wiki-Template/更改各种路径","h":"#packagejson","p":13},{"i":20,"t":"// 你只需要改底下那个\"点击查看文档\" function HomepageHeader() { const {siteConfig} = useDocusaurusContext(); return ( <header className={clsx('hero hero--primary', styles.heroBanner)}> <div className=\"container\"> <h1 className=\"hero__title\">{siteConfig.title}</h1> <p className=\"hero__subtitle\">{siteConfig.tagline}</p> <div className={styles.buttons}> <Link className=\"button button--secondary button--lg\" to=\"/intro\"> 点击查看文档 </Link> </div> </div> </header> ); }","s":"src\\pages\\index.js","u":"/Wiki-Template/更改各种路径","h":"#srcpagesindexjs","p":13},{"i":22,"t":"const FeatureList = [ { title: '简单易用', Svg: require('@site/static/img/undraw_docusaurus_mountain.svg').default, description: ( <> 就这点配置有啥难的. </> ), }, { title: '文档详细', Svg: require('@site/static/img/undraw_docusaurus_tree.svg').default, description: ( <> 看完文档学不会怎么用说明你是个傻逼. </> ), }, { title: '功能强大', Svg: require('@site/static/img/undraw_docusaurus_react.svg').default, description: ( <> 多说无益, 用用就知道了. </> ), }, ];","s":"src\\components\\HomepageFeatures\\index.js","u":"/Wiki-Template/更改各种路径","h":"#srccomponentshomepagefeaturesindexjs","p":13},{"i":24,"t":"jobs: # 自动生成PDF pdf: # 生成PDF - name: Build PDF # neige7换成你的id, XXX-Wiki改成项目名称 run: sudo docker run --rm -v ${{ github.workspace }}/pdf:/app/pdf iseason/docusaurus-to-pdf -u https://neige7.github.io/XXX-Wiki/intro --output pdf/XXX-Wiki.pdf -u https://neige7.github.io/XXX-Wiki/en/intro --output pdf/XXX-Wiki-English.pdf # Release release: # 向release上传XXX-Wiki.pdf - name: Upload PDF file to Release with: # XXX-Wiki改成项目名称 asset_path: XXX-Wiki.pdf asset_name: XXX-Wiki.pdf","s":".github\\workflows\\build.yml","u":"/Wiki-Template/更改各种路径","h":"#githubworkflowsbuildyml","p":13},{"i":26,"t":"为师全帮你写好了, 你每次往上面提交完东西, 他就会自动更新网页, 生成pdf并发布","s":"自动生成pdf文件","u":"/Wiki-Template/自动生成pdf文件","h":"","p":25},{"i":28,"t":"1233211234567","s":"测试","u":"/Wiki-Template/折叠示例2/测试","h":"","p":27},{"i":30,"t":"回到XXX-Wiki仓库页面 点击右上角的Settings按钮 点击左侧的Pages按钮 选择开启的分支 选择gh-pages分支 点击Save按钮 上面的就是你的网页链接","s":"自动生成网页","u":"/Wiki-Template/自动生成网页","h":"","p":29},{"i":32,"t":"回到XXX-Wiki仓库页面 点击右上角的Settings按钮 点击左侧的Actions按钮 点击General按钮 翻到最下方的Workflow permissions部分 勾选Read and write permissions 勾选Allow GitHub Actions to create and approve pull requests 点击Save按钮","s":"设置Action权限","u":"/Wiki-Template/设置Action权限","h":"","p":31},{"i":34,"t":"点击右上角你的头像 点击下方的Settings按钮 点击左下方的Developer settings按钮 点击左侧的Personal access tokens按钮 点击Tokens (classic)按钮 点击右上方的Generate new token按钮 点击Generate new token (classic)按钮 Note里填用途, 用于提醒你这是干什么的, 你可以直接填项目名称, 比如XXX-Wiki 将其设置为永不过期 勾选repo下的所有选项 点击最下方的Generate token按钮 复制生成的token(关闭页面后你就再也无法查看它了) 回到XXX-Wiki仓库页面 点击右上角的Settings按钮 点击左下方的Secrets and variables按钮 点击Actions按钮 点击右上角绿色的New repository secret按钮 Name填写ACCESS_TOKEN, Secret填写你刚才复制的东西 点击下方绿色的Add secret按钮","s":"设置ACCESS_TOKEN","u":"/Wiki-Template/设置ACCESS_TOKEN","h":"","p":33}],"index":{"version":"2.3.9","fields":["t"],"fieldVectors":[["t/2",[0,1.486,1,2.712,2,2.023,3,1.513,4,3.626,5,1.294,6,2.712,7,1.787,8,1.787,9,2.712,10,2.155,11,2.712,12,2.155,13,2.155,14,2.